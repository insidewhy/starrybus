{
  "name": "starrybus",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "jest",
    "build": "tsc -p src",
    "typecheck": "npm run build -- --noEmit",
    "watch": "npm run build -w",
    "clean": "rimraf dist",
    "lint": "eslint 'src/*.ts'",
    "lint-fix": "npm run lint -- --fix",
    "run-prettier": "prettier 'src/*.ts' '*.md'",
    "validate-prettiness": "npm run run-prettier -- -c",
    "make-prettier": "npm run run-prettier -- --write",
    "validate": "npm run test && npm run lint && npm run validate-prettiness"
  },
  "author": "insidewhy <github@chilon.net>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.16.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
